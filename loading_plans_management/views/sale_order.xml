<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="migration_sale_order_form_inherit" model="ir.ui.view">
            <field name="name">migration.sale.order.form</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='tag_ids']" position="after">
                    <field name="session_id" />
                    <field name="car_id" readonly="1"/>
                    <field name="is_concrete" readonly="1" />
                    <field name="open_order" readonly="1"/>
                    <field name="loading_request_id" readonly="1"/>

                </xpath>
                
            </field>
        </record>
    </data>
</odoo>